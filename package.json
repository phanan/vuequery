{
  "name": "VueQuery",
  "version": "1.0.0",
  "description": "Traverse Vue's component tree with ease",
  "main": "dist/vuequery.min.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint src/**/*.js",
    "build": "cross-env NODE_ENV=production && yarn lint && webpack && uglifyjs dist/vuequery.js -cm --comments -o dist/vuequery.min.js",
    "test": "cross-env NODE_ENV=test && yarn lint && webpack && karma start test/unit/karma.conf.js --single-run"
  },
  "keywords": [
    "vue",
    "vue.js",
    "vuejs",
    "component",
    "components",
    "traverse",
    "query"
  ],
  "author": "Phan An <me@phanan.net> (http://phanan.net)",
  "license": "MIT",
  "dependencies": {
    "vue": "^2.1.6"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.0.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.9",
    "babel-preset-es2015": "^6.18.0",
    "chai": "^3.5.0",
    "cross-env": "^3.1.4",
    "eslint": "^3.12.1",
    "eslint-config-vue": "^2.0.1",
    "eslint-plugin-vue": "^1.0.0",
    "karma": "^1.3.0",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sinon-chai": "^1.2.4",
    "karma-webpack": "^1.8.0",
    "mocha": "^3.2.0",
    "phantomjs-prebuilt": "^2.1.14",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "uglify-js": "^2.7.5",
    "webpack": "^1.14.0"
  }
}
